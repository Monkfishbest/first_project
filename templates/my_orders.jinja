{% extends "customer_base.jinja" %} {% block content %}
<h2>My orders</h2>
<div class="grid-container">
    {% for order in orders %}
        <ul class="grid-item">
            <li>Order ID: {{ order.id }}</li>
            <li>Frame: {{ order.frame.style }}</li>
            <li>Frame size: {{ order.frame.size }}</li>
            <li>Fabric: {{ order.fabric.pattern }}</li>
        </ul>
    {% endfor %}
</div>
{% endblock %}